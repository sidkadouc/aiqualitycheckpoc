{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "envType": {
      "value": "${AZURE_ENV_TYPE=dev}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "managedIdentityId": {
      "value": "${AZURE_MANAGED_IDENTITY_ID}"
    },
    "managedIdentityClientId": {
      "value": "${AZURE_MANAGED_IDENTITY_CLIENT_ID}"
    },
    "logAnalyticsCustomerId": {
      "value": "${AZURE_LOG_ANALYTICS_CUSTOMER_ID}"
    },
    "logAnalyticsSharedKey": {
      "value": "${AZURE_LOG_ANALYTICS_SHARED_KEY}"
    },
    "appInsightsConnectionString": {
      "value": "${AZURE_APPLICATION_INSIGHTS_CONNECTION_STRING}"
    },
    "keyVaultName": {
      "value": "${AZURE_KEY_VAULT_NAME}"
    },
    "containerRegistryName": {
      "value": "${AZURE_CONTAINER_REGISTRY_NAME}"
    },
    "containerRegistryEndpoint": {
      "value": "${AZURE_CONTAINER_REGISTRY_ENDPOINT}"
    },
    "azureOpenAIEndpoint": {
      "value": "${AZURE_OPENAI_ENDPOINT=}"
    },
    "azureOpenAIChatModel": {
      "value": "${AZURE_OPENAI_CHAT_MODEL=gpt-4.1}"
    },
    "embeddingModel": {
      "value": "${AZURE_EMBEDDING_MODEL=text-embedding-3-large}"
    },
    "azureAISearchUri": {
      "value": "${AZURE_AISEARCH_ENDPOINT=}"
    },
    "aiSearchIndexerName": {
      "value": "${AISEARCH_INDEXER_NAME=vector-jt-poc-indexer}"
    },
    "contentUnderstandingEndpoint": {
      "value": "${AZURE_CONTENT_UNDERSTANDING_ENDPOINT=}"
    },
    "azureAIFoundryEndpoint": {
      "value": "${AZURE_AI_FOUNDRY_ENDPOINT=}"
    },
    "enableDatabaseSeeding": {
      "value": "${ENABLE_DATABASE_SEEDING=true}"
    },
    "tenantId": {
      "value": "${AZURE_TENANT_ID}"
    },
    "senderEmail": {
      "value": "${EMAIL_SENDER=contact@yaleaf.com}"
    },
    "emailTenantId": {
      "value": "${EMAIL_TENANT_ID=}"
    },
    "frontendBaseUrl": {
      "value": "${FRONTEND_BASE_URL=}"
    }
  }
}
